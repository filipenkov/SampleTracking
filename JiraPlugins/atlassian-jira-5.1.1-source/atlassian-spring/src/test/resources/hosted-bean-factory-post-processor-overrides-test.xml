<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <!-- This should override the bean in the other context file -->
    <bean id="overridableBean" class="com.atlassian.spring.hosted.OverriddenOverridableBean"/>

    <bean id="notOverriddenBean" class="com.atlassian.spring.hosted.DefaultOverridableBean"/>

</beans>