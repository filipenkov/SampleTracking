<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>atlassian-closedsource-pom</artifactId>
        <groupId>com.atlassian.pom</groupId>
        <version>1</version>
    </parent>
    <groupId>com.atlassian.renderer</groupId>
    <artifactId>atlassian-renderer</artifactId>
    <name>Atlassian Renderer</name>
    <version>3.14.7</version>
    <description>The library that renders wiki markup for Confluence and JIRA.</description>
    <scm>
        <connection>scm:svn:https://svn.atlassian.com/svn/private/atlassian/atlassian-renderer/tags/atlassian-renderer-3.14.7</connection>
        <developerConnection>scm:svn:https://svn.atlassian.com/svn/private/atlassian/atlassian-renderer/tags/atlassian-renderer-3.14.7</developerConnection>
        <url>https://svn.atlassian.com/svn/private/atlassian/atlassian-renderer/tags/atlassian-renderer-3.14.7</url>
    </scm>
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <includes>
                        <include>**/Test*.java</include>
                        <include>**/*Test.java</include>
                    </includes>
                    <excludes>
                        <exclude>**/*$*.java</exclude>
                        <exclude>**/FunctionalTest.java</exclude>
                    </excludes>
                </configuration>
            </plugin>
        </plugins>
    </build>
