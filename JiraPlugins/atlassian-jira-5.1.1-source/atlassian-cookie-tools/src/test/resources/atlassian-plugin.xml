<atlassian-plugin key="com.atlassian.security.atlassian-cookie-tools-test" name="Atlassian Cookie Tools Test Plugin" pluginsVersion="2">
    <plugin-info>
        <version>${project.version}</version>
        <vendor name="Atlassian Software Systems Pty Ltd" url="http://www.atlassian.com/"/>
    </plugin-info>

    <servlet name="Cookie Test Servlet" key="cookieTestServlet" class="com.atlassian.security.cookie.CookieTestServlet">
        <url-pattern>/cookies</url-pattern>
    </servlet>
</atlassian-plugin>
