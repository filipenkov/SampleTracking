            <build>
                <plugins>
                    <plugin>
                        <groupId>com.atlassian.maven.plugins</groupId>
                        <artifactId>maven-amps-plugin</artifactId>
                        <version>${amps.version}</version>
                        <extensions>true</extensions>
                        <configuration>
                            <instanceId>studio</instanceId>
                            <products>
                                <product>
                                    <id>studio</id>
                                    <instanceIds>
                                        <instanceId>studio-crowd</instanceId>
                                        <instanceId>studio-jira</instanceId>
                                        <instanceId>studio-confluence</instanceId>
                                        <instanceId>studio-fecru</instanceId>
                                    </instanceIds>
                                    <version>${studio.version}</version>
                                    <dataVersion>${studio.data.version}</dataVersion>
                                    <enableFastdev>false</enableFastdev>
                                    <enableDevToolbox>false</enableDevToolbox>
                                </product>
                                <product>
                                    <id>studio-jira</id>
                                    <!--pluginArtifacts>
                                        <pluginArtifact>
                                            <groupId>com.atlassian.jira</groupId>
                                            <artifactId>jira-func-test-plugin</artifactId>
                                            <version>${jira.version}</version>
                                        </pluginArtifact>
                                    </pluginArtifacts-->
                                    <jvmArgs>-Xmx1600m</jvmArgs>
                                    <enableFastdev>false</enableFastdev>
                                    <enableDevToolbox>false</enableDevToolbox>
                                </product>
                                <product>
                                    <id>studio-confluence</id>
                                    <groupId>com.atlassian.confluence</groupId>
                                    <artifactId>confluence-studio-webapp</artifactId>
                                    <version>${confluence.version}</version>
                                </product>
                            </products>
                            <systemPropertyVariables>
                                <webdriver.browser>${test.browser}</webdriver.browser>
                                <xvfb.enable>${xvfb.enable}</xvfb.enable>
                                <jira.xml.data.location>${build.testOutputDirectory}/xml</jira.xml.data.location>
                                <jira.port>${jira.http.port}</jira.port>
                                <atlassian.test.target.dir>${project.build.directory}/test-reports</atlassian.test.target.dir>
                            </systemPropertyVariables>
                        </configuration>
                    </plugin>
                    <plugin>
                        <artifactId>maven-dependency-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>unpack-jira-tests</id>
                                <phase>pre-integration-test</phase>
                                <goals>
                                    <goal>unpack-dependencies</goal>
                                </goals>
                                <configuration>
                                    <excludeTransitive>true</excludeTransitive>
                                    <includeArtifactIds>jira-webdriver-tests</includeArtifactIds>
                                    <outputDirectory>${project.build.directory}/test-classes</outputDirectory>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
