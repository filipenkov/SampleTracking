                <studio.data.version>115-SNAPSHOT</studio.data.version>
                <amps.version>3.8-SNAPSHOT</amps.version>
                <test.browser>firefox</test.browser>
                <xvfb.enable>false</xvfb.enable>
                <host.name>localhost</host.name>
                <jira.http.port>2990</jira.http.port>
                <jira.http.context>jira</jira.http.context>
                <jira.test.lib.version>${jira.version}</jira.test.lib.version>
                <confluence.version>4.1.3-studio-5</confluence.version>
