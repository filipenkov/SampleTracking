  <xs:complexType name="IssueTypeType">
    <xs:annotation>
      <xs:documentation>An entity to represent custom </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="description_prototype">
        <xs:attribute type="xs:short" name="id" use="required">
          <xs:annotation>
            <xs:documentation>
              The ID of this resolution, usually just an integer.
              It is a varchar field for a) backwards compatibility and b) data import from other systems.
              This means the PK must be created manually.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="ordinal_marker" name="sequence" use="optional">
          <xs:annotation>
            <xs:documentation>the sequence of this type. 1 is high, > 1 is low.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" name="name" use="required"/>
        <xs:attribute type="xs:string" name="iconurl" use="optional"><!-- /images/icons/....(gif/png) -->
          <xs:annotation>
            <xs:documentation>
              relative URL of an icon for this issue type - must be 16x16
              This is present for all of the entries in the backup but it is not tested for in the validation code.
            </xs:documentation>
            <xs:appinfo>always</xs:appinfo>
          </xs:annotation>
        </xs:attribute>        
        <xs:attribute type="xs:string" name="style" use="optional" fixed="jira_subtask">
          <xs:annotation>
            <xs:documentation>
              the 'type' of this issue type
              currently the code only tests if this is equals to "jira_subtask" or not. (case sensitive)
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
