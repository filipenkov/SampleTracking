        <xs:attribute type="xs:short" name="eventTypeId" use="required"/><!-- ref -->
        <xs:attribute type="NotificationType_type_enum" name="type" use="required"/>
        <xs:attribute type="xs:string" name="parameter" use="optional">
          <xs:annotation>
            <xs:documentation>
              The type of value stored in parameter is dependent on the value of type:
              type attribute           parameter value's type
              All_Watchers             No parameter
              Current_Assignee         No parameter
              Current_Reporter         No parameter
              Group_Dropdown           user group
              Project_Role             project number ([0-9]{5})
              User_Custom_Field_Value  'customfield_'[0-9]{5}              
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" name="event" use="optional">
          <xs:annotation>
            <xs:documentation>
              Only in entitymodel.xml, not seen in the backup.
              Deprecated eventTypeId is now used
            </xs:documentation>
            <xs:appinfo>deprecated</xs:appinfo>
          </xs:annotation>
        </xs:attribute>        
        <xs:attribute type="xs:short" name="templateId" use="optional"><!--ref-->
          <xs:annotation>
            <xs:documentation>
              Only in entitymodel.xml, not seen in the backup.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>                
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

