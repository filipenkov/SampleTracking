  <xs:complexType name="ActionType"> <!-- mixed="true" -->
    <xs:annotation>
      <xs:documentation>Used to store 'Comment' objects.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element type="xs:string" name="body" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute type="xs:int" name="id" use="required"/>
    <xs:attribute type="xs:int" name="issue" use="required"/>
    <xs:attribute type="xs:string" name="author" use="required">
      <xs:annotation><xs:appinfo>always</xs:appinfo></xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="type" use="required" fixed="comment">
      <xs:annotation>
        <xs:documentation>
          Other values are not necessarily errors but the contents of any Actions without this attribute or
          with it set to anything but 'comment' are ignored.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="created" use="optional">
      <xs:annotation><xs:appinfo>always</xs:appinfo></xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="updateauthor" use="optional">
      <xs:annotation><xs:appinfo>always</xs:appinfo></xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="updated" use="optional">
      <xs:annotation><xs:appinfo>always</xs:appinfo></xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" name="body"/>
    <xs:attribute type="xs:string" name="level">
      <xs:annotation>
        <xs:documentation>
          If set this is used to limit visibility of the comment to the named usergroup 
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:short" name="rolelevel" use="optional">
      <xs:annotation>
        <xs:documentation>
          If set this is used to limit visibility of the comment to users with the referenced ProjectRole.id  
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:int" name="numvalue" use="optional"><xs:annotation>
      <xs:documentation>
        Only in entitymodel.xml, not seen in the backup.
        an arbitrary number that can be attached to actions used for time log
      </xs:documentation>
    </xs:annotation></xs:attribute>
  </xs:complexType>

