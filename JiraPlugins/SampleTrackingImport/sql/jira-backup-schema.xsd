  <xs:complexType name="PriorityType">
    <xs:complexContent>
      <xs:extension base="description_prototype">        <!-- "description" required? -->
        <xs:attribute type="xs:short" name="id" use="required">
          <xs:annotation>
            <xs:documentation>
              The ID of this priority, usually just an integer.
              It is a varchar field for a) backwards compatibility and b) data import from other systems.
              This means the PK must be created manually.
          </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="ordinal_marker" name="sequence" use="required">
          <xs:annotation>
            <xs:documentation>the sequence of this priority. 1 is high, > 1 is low.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" name="name" use="required"/>
        <xs:attribute type="xs:string" name="iconurl" use="required"><!-- /<path>/<image file>.(gif/?) -->
          <xs:annotation>
            <xs:documentation>relative URL of an icon for this priority - must be 16x16</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" name="statusColor" use="required"><!-- type: #[0-9a-h]{6} -->
          <xs:annotation>
            <xs:documentation>hex string or the color for this priority</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

