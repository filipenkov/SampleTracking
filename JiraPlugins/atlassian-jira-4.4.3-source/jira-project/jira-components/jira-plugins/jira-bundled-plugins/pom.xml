        <plugins>
            <plugin>
                <groupId>com.luuuis</groupId>
                <artifactId>smartass-maven-plugin</artifactId>
                <version>1.0</version>
                <executions>
                    <execution>
                        <id>create-bundled-plugins-zip</id>
                        <phase>package</phase>
                        <goals>
                            <goal>archive-dependencies</goal>
                        </goals>
                        <configuration>
                            <includeScope>runtime</includeScope>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <finalName>atlassian-bundled-plugins</finalName>
